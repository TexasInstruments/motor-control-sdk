%%{
    let module = system.modules[args[0]];
%%}
/*
 * SDFM
 */

/* SDFM Instance Macros */
#define CONFIG_SDFM_NUM_INSTANCES               `module.$instances.length`
%for(let i = 0; i < module.$instances.length; i++) {
    % let instance = module.$instances[i];
    % let Slice = 0 ;
    %if(instance.instance === "ICSSG1")
    %{
       % if(instance.PRU_ICSSG1_PRU.$assign === "PRU_ICSSG1_PRU1")
       %{
          % Slice = 1;
       %}

    %}
    %else
    %{
       %if(instance.PRU_ICSSG0_PRU.$assign === "PRU_ICSSG0_PRU1")
       %{
          % Slice = 1;
       %}

    %}
    
#define `instance.$name`                  `i`
#define `instance.$name`_CHANNEL0         `instance.Enable_Channel_0 & 1`
#define `instance.$name`_CHANNEL1         `instance.Enable_Channel_1 & 1`
#define `instance.$name`_CHANNEL2         `instance.Enable_Channel_2 & 1`
#define `instance.$name`_CHANNEL3         `instance.Enable_Channel_3 & 1`
#define `instance.$name`_CHANNEL4         `instance.Enable_Channel_4 & 1`
#define `instance.$name`_CHANNEL5         `instance.Enable_Channel_5 & 1`
#define `instance.$name`_CHANNEL6         `instance.Enable_Channel_6 & 1`
#define `instance.$name`_CHANNEL7         `instance.Enable_Channel_7 & 1`
#define `instance.$name`_CHANNEL8         `instance.Enable_Channel_8 & 1`
#define `instance.$name`_SLICE            `Slice`
% }
